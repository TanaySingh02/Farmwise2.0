{
  "name": "farmwise",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc -b",
    "start": "node dist/src/index",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/src/index\"",
    "db:generate": "drizzle-kit generate --schema src/db/schema.ts --dialect postgresql --out ./drizzle",
    "db:migrate": "tsx ./src/scripts/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "download-files": "bun run build && node dist/src/agents/core-profile-agent.js download-files",
    "dev-agent": "tsx ./src/agents/core-profile-agent.ts dev",
    "dev-agent2": "tsx ./src/agents/log-automation-agent.ts dev",
    "start:worker": "node --import tsx ./src/bullmq/worker-runner.ts"
  },
  "dependencies": {
    "@clerk/express": "^1.7.38",
    "@langchain/core": "^1.0.0-alpha.7",
    "@langchain/google-genai": "^1.0.0",
    "@langchain/groq": "^1.0.0",
    "@langchain/openai": "^1.0.0-alpha.3",
    "@langchain/qdrant": "^1.0.0",
    "@livekit/agents": "^1.0.11",
    "@livekit/agents-plugin-cartesia": "^1.0.11",
    "@livekit/agents-plugin-deepgram": "^1.0.11",
    "@livekit/agents-plugin-elevenlabs": "^1.0.11",
    "@livekit/agents-plugin-google": "^1.0.11",
    "@livekit/agents-plugin-livekit": "^1.0.11",
    "@livekit/agents-plugin-neuphonic": "^1.0.11",
    "@livekit/agents-plugin-openai": "^1.0.11",
    "@livekit/agents-plugin-resemble": "^1.0.11",
    "@livekit/agents-plugin-rime": "^1.0.4",
    "@livekit/agents-plugin-silero": "^1.0.11",
    "@neondatabase/serverless": "^1.0.2",
    "@openai/agents": "^0.3.0",
    "@toon-format/toon": "^0.7.3",
    "@types/ioredis": "^5.0.0",
    "@upstash/redis": "^1.35.6",
    "bullmq": "^5.63.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "express": "^5.1.0",
    "ioredis": "^5.8.2",
    "langchain": "^1.0.0-alpha.9",
    "livekit-server-sdk": "^2.14.0",
    "openai": "^6.8.1",
    "openmeteo": "^1.2.2",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.2",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.5",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.6"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
